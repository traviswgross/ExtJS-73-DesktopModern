{
   "name": "Everlast",
   "namespace": "Everlast",
   "version": "1.0.0.0",
   "framework": "ext",
   "toolkit": "modern",
   "theme": "theme-material-6140e4cf-594d-4333-88fb-2c08cb3a47d6",
   "requires": [
      "font-awesome"
   ],
   "indexHtmlPath": "index.html",
   "classpath": [
      "app"
   ],
   "overrides": [
      "overrides"
   ],
   "fashion": {
      "missingParameters": "error",
      "inliner": {
         "enable": false
      }
   },
   "sass": {
      "namespace": "Everlast",
      "generated": {
         "var": "sass/save.scss",
         "src": "sass/save"
      },
      "etc": [
         "sass/etc/all.scss"
      ],
      "var": [
         "sass/var/all.scss",
         "sass/var"
      ],
      "src": [
         "sass/src"
      ]
   },
   "js": [
      {
         "path": "${framework.dir}/build/ext-modern-all-debug.js"
      },
      {
         "path": "app.js",
         "bundle": true
      }
   ],
   "css": [
      {
         "path": "${build.out.css.path}",
         "bundle": true,
         "exclude": [
            "fashion"
         ]
      }
   ],
   "loader": {
      "cache": false,
      "cacheParam": "_dc"
   },
   "production": {
      "output": {
         "appCache": {
            "enable": false,
            "path": "cache.appcache"
         }
      },
      "loader": {
         "cache": "${build.timestamp}"
      },
      "cache": {
         "enable": true
      },
      "compressor": {
         "type": "yui"
      }
   },
   "testing": {
      // "baseURL": "http://localhost:8000",
      "watch": {
         "delay": 250
      }
   },
   "development": {
      // "baseURL": "http://localhost:8000",
      "watch": {
         "delay": 250
      }
   },
   "bootstrap": {
      "base": "${app.dir}",
      "microloader": "bootstrap.js",
      "css": "bootstrap.css"
   },
   "output": {
      "base": "${workspace.build.dir}/${build.environment}/${app.name}",
      "appCache": {
         "enable": false
      }
   },
   "cache": {
      "enable": false,
      "deltas": true
   },
   "appCache": {
      "cache": [
         "index.html"
      ],
      "network": [
         "*"
      ],
      "fallback": []
   },
   "resources": [
      {
         "path": "resources",
         "output": "shared"
      }
   ],
   "archivePath": "archive",
   "slicer": null,
   "ignore": [
      "(^|/)CVS(/?$|/.*?$)"
   ],
   "id": "51b9a576-c9ea-4360-a348-7ee4344e8dcf"
}